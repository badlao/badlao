{
  "kind": "collectionType",
  "collectionName": "loan_applications",
  "info": {
    "singularName": "loan-application",
    "pluralName": "loan-applications",
    "displayName": "Loan Application",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "form_no": {
      "type": "integer"
    },
    "member_no": {
      "type": "integer"
    },
    "group_no": {
      "type": "integer"
    },
    "date_of_become_member": {
      "type": "date"
    },
    "applicant_photos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "applicant_name": {
      "type": "string"
    },
    "mother_name": {
      "type": "string"
    },
    "father_or_spouse_name": {
      "type": "string"
    },
    "current_address": {
      "type": "text"
    },
    "permanent_address": {
      "type": "text"
    },
    "national_id": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    },
    "education_level": {
      "type": "enumeration",
      "enum": [
        "primary",
        "secondary",
        "hsc",
        "graduate",
        "postgraduate",
        "none"
      ]
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "other"
      ]
    },
    "marital_status": {
      "type": "enumeration",
      "enum": [
        "single",
        "married",
        "widow"
      ]
    },
    "loan_purpose": {
      "type": "text"
    },
    "loan_no": {
      "type": "integer"
    },
    "user_of_loan": {
      "type": "enumeration",
      "enum": [
        "SELF",
        "SPOUSE",
        "CHILD",
        "OTHER"
      ]
    },
    "business_type": {
      "type": "enumeration",
      "enum": [
        "NEW",
        "OLD"
      ]
    },
    "loanee_name": {
      "type": "string"
    },
    "loanee_nid": {
      "type": "string"
    },
    "with_multiple_installments": {
      "type": "boolean",
      "default": false
    },
    "loan_amount_requested": {
      "type": "integer"
    },
    "loan_duration": {
      "type": "integer"
    },
    "installment_amount": {
      "type": "integer"
    },
    "loan_duration_unit": {
      "type": "enumeration",
      "enum": [
        "DAYS",
        "WEEK",
        "MONTH",
        "YEAR"
      ]
    },
    "total_installments": {
      "type": "integer"
    },
    "installment_duration": {
      "type": "integer"
    },
    "installment_duration_unit": {
      "type": "enumeration",
      "enum": [
        "DAYS",
        "WEEKLY",
        "MONTHLY",
        "YEARLY"
      ]
    },
    "mobile_banking_type": {
      "type": "enumeration",
      "enum": [
        "BKASH",
        "NAGAD",
        "ROCKET",
        "OTHER"
      ]
    },
    "mobile_banking_acc_no": {
      "type": "string"
    },
    "has_bank_account": {
      "type": "boolean"
    },
    "bank": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bank.bank"
    },
    "bank_branch": {
      "type": "string"
    },
    "bank_account_title": {
      "type": "string"
    },
    "bank_account_number": {
      "type": "string"
    },
    "application_date": {
      "type": "date"
    },
    "applicant_signature": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "loanee_nid_doc": {
      "type": "media",
      "multiple": false,
      "required": false
    },
    "applicant_nid_doc": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "guarantors_document": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "social_evaluation": {
      "type": "component",
      "component": "shared.social-evaluation",
      "repeatable": false
    },
    "approved_by": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "loan_status": {
      "type": "enumeration",
      "enum": [
        "DRAFT",
        "FORWARDED_FOR_APPROVAL",
        "IN_PROGRESS",
        "APPROVED",
        "REJECTED"
      ]
    },
    "loan_acceptance": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::loan-acceptance.loan-acceptance",
      "inversedBy": "loan_applications"
    },
    "installments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::installment.installment",
      "mappedBy": "loan_application"
    },
    "bail_bonds": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bail-bond.bail-bond",
      "mappedBy": "loan_application"
    },
    "holofnamas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::holofnama.holofnama",
      "mappedBy": "loan_application"
    }
  }
}
